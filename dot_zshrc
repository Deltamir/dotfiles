# Path to Oh My Zsh
export ZSH="$HOME/.oh-my-zsh"
export ZSH_CUSTOM="$HOME/.zsh-custom"


# Theme fallback (real prompt in $ZSH_CUSTOM/prompt.zsh)
ZSH_THEME="robbyrussell"


# OMZ plugins
plugins=(git git-prompt docker kubectl aws gcloud debian pip python colored-man-pages command-not-found virtualenv)


source $ZSH/oh-my-zsh.sh


# Load custom configs
for file in $ZSH_CUSTOM/*.zsh; do
  source "$file"
done

# Created by `pipx` on 2025-12-11 23:53:54
export PATH="$PATH:/home/debian/.local/bin"

eval "$(/home/linuxbrew/.linuxbrew/bin/brew shellenv)"
